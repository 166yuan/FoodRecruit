<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.recruit.competition.model.Team" table="team">
		<id name="id">
		 <generator class="identity"/>
		</id>
		<property name="name" not-null="true" unique  ="true"></property>
		<property name="size"  ></property>
        <property name="slogan"  ></property>
        <property name="leader_id"  not-null="true"></property>
        <property name="password"  not-null="true"></property>
        <property name="competition_id"  not-null="true"></property>
		</class>
</hibernate-mapping>
